<?xml version="1.0" encoding="UTF-8"?>
<emfmapping:EntityMappingsType
	xmi:version="2.0"
	xmlns:xmi="http://www.omg.org/XMI"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:emfmapping="https://gecko.org/persistence/jpa/emfmapping"
	xsi:schemaLocation="https://gecko.org/persistence/jpa/emfmapping emfmapping.ecore">
	<entity>
		<table name="otherTable" />
		<secondaryTable
			name="secondaryTable" />
		<class xsi:type="ecore:EClass"
			href="http://simple.simple/simple#//ClassOne" />
		<attributes>
			<basic>
				<column
					name="one" />
				<name
					xsi:type="ecore:EAttribute"
					href="http://simple.simple/simple#//ClassOne/attributeOne" />
			</basic>
			<basic>
				<column
					name="two" table="secondaryTable" />
				<name
					xsi:type="ecore:EAttribute"
					href="http://simple.simple/simple#//ClassOne/attributeTwo" />
			</basic>
			<basic>
				<column
					name="three" table="otherTable" />
				<name
					xsi:type="ecore:EAttribute"
					href="http://simple.simple/simple#//ClassOne/attributeThree" />
			</basic>
		</attributes>
	</entity>
</emfmapping:EntityMappingsType>
