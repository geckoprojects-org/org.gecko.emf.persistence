<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.1" >

    <entity class="example.orm.Dog" access="VIRTUAL">
        <table name="dogTable" catalog="thecatalog"/>
        <attributes>
            <id name="id" attribute-type="String">
                <column name="dbid" precision="-1"/>
            </id>
            <basic name="name" attribute-type="String" >
                <column name="dbname" nullable="false" length="30"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
